{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "pages/*": ["./src/pages/*"],
      "components/*": ["./src/components/*"],
      "utils/*": ["./src/utils/*"],
      "assets/*": ["./src/assets/*"],
      "routes/*": ["./src/routes/*"],
      "store/*": ["./src/store/*"],
      "styles/*": ["./src/styles"],
      "helpers/*": ["./src/helpers/*"],
      "src/*": ["./src/*"],
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
